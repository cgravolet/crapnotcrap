{
  "name": "crapnotcrap",
  "version": "1.0.0",
  "description": "A website for deciding what is crap and what is not crap.",
  "repository": {
    "type": "git",
    "url": "https://github.com/cgravolet/crapnotcrap.git"
  },
  "private": true,
  "scripts": {
    "backup": "mongodump -d crapnotcrap -o database_snapshot",
    "crawlpolls": "node crawler/polls.js",
