  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "express": "^4.0.0",
    "morgan": "^1.0.0",
    "cookie-parser": "^1.0.1",
    "body-parser": "^1.0.0",
    "debug": "^0.7.4",
    "jade": "^1.3.0",
    "stylus": "0.42.3",
    "jsdom": "^3.0.0",
    "mongodb": "*",
    "request": "*"
  }
}
